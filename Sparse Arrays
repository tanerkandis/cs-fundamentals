
    public static List<Integer> matchingStrings(List<String> stringList, List<String> queries) {
    // Write your code here
    List<Integer> repeatedList = new ArrayList<>();
        for(String quaryString : queries){
            int repeatedWords = 0;
            for(String inpuString : stringList){
                if(quaryString.equals(inpuString)){
                    repeatedWords++;
                }
            }
            repeatedList.add(repeatedWords);
        }
        return repeatedList;
    }
